<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_user_graph"
    app:startDestination="@id/Second2Fragment">

    <fragment
        android:id="@+id/First2Fragment"
        android:name="com.example.studentrelief.ui.user.UserFormFragment_"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_user_form">
        <argument
            android:name="myArg"
            app:argType="integer"
            android:defaultValue="1" />
        <action
            android:id="@+id/action_First2Fragment_to_Second2Fragment"
            app:destination="@id/Second2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/Second2Fragment"
        android:name="com.example.studentrelief.ui.user.Second2Fragment_"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second2">
        <argument
            android:name="myArg"
            app:argType="integer"
            android:defaultValue="0" />
        <action
            android:id="@+id/action_Second2Fragment_to_First2Fragment"
            app:destination="@id/First2Fragment" />
    </fragment>
</navigation>