<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_course"
    app:startDestination="@id/fragment_course_list">

    <fragment
        android:id="@+id/fragment_course_list"
        android:name="com.example.studentrelief.ui.course.CourseListFragment_"
        android:label="@string/label_course_list"
        tools:layout="@layout/fragment_course_list">
        <action
            android:id="@+id/action_fragment_course_list_to_fragment_course_form"
            app:destination="@id/fragment_course_form" />
    </fragment>
    <fragment
        android:id="@+id/fragment_course_form"
        android:name="com.example.studentrelief.ui.course.CourseFormFragment_"
        android:label="@string/label_course_form"
        tools:layout="@layout/fragment_course_form">
        <argument
            android:name="courseID"
            app:argType="integer"
            android:defaultValue="0" />
        <action
            android:id="@+id/action_fragment_course_form_to_fragment_course_list"
            app:destination="@id/fragment_course_list" />

    </fragment>
</navigation>